<COMPONENT>
    <HEADER
		PLATEFORM="ALL"
		SERIAL=""
		VERSION="0.101"
		STATUS="ALPHA"
		COMPATIBILITY="ALL"
		AUTHOR="Talend"
		RELEASE_DATE="20170608A"
	    STARTABLE="false"
	    TECHNICAL="true"
	    SCHEMA_AUTO_PROPAGATE="true"
	    DATA_AUTO_PROPAGATE="false"
		PARTITIONING="NONE"
        LOG4J_ENABLED="true"
	>
        <SIGNATURE></SIGNATURE>
    </HEADER>
    <FAMILIES>
        <FAMILY>Databases/DB Specifics/Cosmos DB</FAMILY>
        <FAMILY>Cloud/Azure/Cosmos DB</FAMILY>
    </FAMILIES>
	<DOCUMENTATION>
        <URL />
	</DOCUMENTATION>
	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
        <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0" />
        <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
        <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
        <CONNECTOR CTYPE="COMPONENT_OK" />
        <CONNECTOR CTYPE="COMPONENT_ERROR" />
        <CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>
	<PARAMETERS>
		<PARAMETER
            NAME="USE_EXISTING_CONNECTION"
            FIELD="CHECK"
            NUM_ROW="2"
            GROUP="CONNECTION"
        >
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER
            NAME="CONNECTION"
            FIELD="COMPONENT_LIST"
            REQUIRED="true" FILTER="tCosmosDBConnection"
            NUM_ROW="2"
            SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
            GROUP="CONNECTION"
        />
        <PARAMETER
            NAME="API"
            FIELD="CLOSED_LIST"
            NUM_ROW="3"
            SHOW_IF="(USE_EXISTING_CONNECTION == 'false')"
            GROUP="CONNECTION"
        >
            <ITEMS DEFAULT="MONGODB">
                <ITEM NAME="MONGODB" VALUE="MONGODB" />
            </ITEMS>
        </PARAMETER>
        <PARAMETER
            FIELD="CHECK"
            NAME="USE_REPLICA_SET"
            NUM_ROW="4"
            SHOW_IF="(USE_EXISTING_CONNECTION == 'false')"
            GROUP="CONNECTION">
            <DEFAULT>"false"</DEFAULT>
        </PARAMETER>
        <PARAMETER
            NAME="REPLICA_SET"
            FIELD="TABLE"
            REQUIRED_IF="(USE_REPLICA_SET=='true')"
            NUM_ROW="5"
            NB_LINES="5"
            SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (USE_REPLICA_SET=='true')"
            GROUP="CONNECTION">
            <ITEMS>
                <ITEM NAME="REPLICA_HOST" FIELD="TEXT" />
                <ITEM NAME="REPLICA_PORT" FIELD="TEXT" />
            </ITEMS>
        </PARAMETER>
        <PARAMETER
            FIELD="TEXT"
            NAME="HOST"
            NUM_ROW="6"
            REQUIRED="true"
            SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (USE_REPLICA_SET=='false')"
            GROUP="CONNECTION"
        >
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER
            FIELD="TEXT"
            NAME="PORT"
            NUM_ROW="6"
            SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (USE_REPLICA_SET=='false')"
            GROUP="CONNECTION"
        >
            <DEFAULT>10255</DEFAULT>
        </PARAMETER>
        <PARAMETER
            FIELD="TEXT"
            NAME="DATABASE"
            NUM_ROW="7"
            REQUIRED="true"
            SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
            GROUP="CONNECTION"
        >
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER FIELD="CHECK" NAME="USE_SSL" NUM_ROW="8" SHOW="false" GROUP="CONNECTION">
            <DEFAULT>true</DEFAULT>
        </PARAMETER>

        <PARAMETER FIELD="CHECK" NAME="SET_WRITE_CONCERN" NUM_ROW="9" GROUP="WRITE_CONCERN_GROUP">
            <DEFAULT>"false"</DEFAULT>
        </PARAMETER>

        <PARAMETER NAME="WRITE_CONCERN" FIELD="CLOSED_LIST" NUM_ROW="10" SHOW_IF="SET_WRITE_CONCERN=='true'" GROUP="WRITE_CONCERN_GROUP">
            <ITEMS DEFAULT="ACKNOWLEDGED">
                <ITEM NAME="ACKNOWLEDGED" VALUE="ACKNOWLEDGED"/>
                <ITEM NAME="UNACKNOWLEDGED" VALUE="UNACKNOWLEDGED"/>
                <ITEM NAME="JOURNALED" VALUE="JOURNALED"/>
                <ITEM NAME="REPLICA_ACKNOWLEDGED" VALUE="REPLICA_ACKNOWLEDGED"/>
            </ITEMS>
        </PARAMETER>

        <PARAMETER FIELD="CHECK" NAME="SET_BULK_WRITE" NUM_ROW="11" GROUP="BULK_WRITE_GROUP">
            <DEFAULT>"false"</DEFAULT>
        </PARAMETER>

        <PARAMETER NAME="BULK_WRITE_TYPE" FIELD="CLOSED_LIST" NUM_ROW="12" SHOW_IF="(SET_BULK_WRITE=='true')" GROUP="BULK_WRITE_GROUP">
            <ITEMS DEFAULT="UNORDERED">
                <ITEM NAME="UNORDERED" VALUE="Unordered"/>
                <ITEM NAME="ORDERED" VALUE="Ordered"/>
            </ITEMS>
        </PARAMETER>

        <PARAMETER FIELD="TEXT" NAME="BULK_WRITE_SIZE" NUM_ROW="13" SHOW_IF="(SET_BULK_WRITE=='true')" GROUP="BULK_WRITE_GROUP">
            <DEFAULT>"1000"</DEFAULT>
        </PARAMETER>

         <PARAMETER
            NAME="REQUIRED_AUTHENTICATION"
            FIELD="CHECK"
            NUM_ROW="15"
            SHOW="false"
            GROUP="AUTHENTICATION"
        >
            <DEFAULT>true</DEFAULT>
        </PARAMETER>

        <PARAMETER NAME="AUTHENTICATION_MECHANISM" FIELD="CLOSED_LIST" NUM_ROW="16"
                SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (REQUIRED_AUTHENTICATION == 'true')" GROUP="AUTHENTICATION">
            <ITEMS DEFAULT="NEGOTIATE_MEC">
                <ITEM NAME="NEGOTIATE_MEC" VALUE="NEGOTIATE_MEC"/>
                <ITEM NAME="PLAIN_MEC" VALUE="PLAIN_MEC"/>
                <ITEM NAME="SCRAMSHA1_MEC" VALUE="SCRAMSHA1_MEC"/>
            </ITEMS>
        </PARAMETER>

        <PARAMETER FIELD="CHECK" NAME="SET_AUTHENTICATION_DATABASE"
            NUM_ROW="18" SHOW_IF="((AUTHENTICATION_MECHANISM=='NEGOTIATE_MEC') OR (AUTHENTICATION_MECHANISM=='SCRAMSHA1_MEC')) AND (REQUIRED_AUTHENTICATION == 'true') AND (USE_EXISTING_CONNECTION == 'false')" GROUP="AUTHENTICATION">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>

        <PARAMETER FIELD="TEXT" NAME="AUTHENTICATION_DATABASE" NUM_ROW="19"
            SHOW_IF="(SET_AUTHENTICATION_DATABASE=='true') AND (isShow[SET_AUTHENTICATION_DATABASE])" GROUP="AUTHENTICATION">
            <DEFAULT>""</DEFAULT>
        </PARAMETER>

        <PARAMETER
            FIELD="TEXT"
            NAME="USERNAME"
            NUM_ROW="20"
            SHOW_IF="((AUTHENTICATION_MECHANISM=='NEGOTIATE_MEC') OR (AUTHENTICATION_MECHANISM=='PLAIN_MEC') OR (AUTHENTICATION_MECHANISM=='SCRAMSHA1_MEC')) AND (REQUIRED_AUTHENTICATION == 'true') AND (USE_EXISTING_CONNECTION == 'false')"
            GROUP="AUTHENTICATION"
        >
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER
            FIELD="PASSWORD"
            NAME="PASSWORD"
            NUM_ROW="20"
            SHOW_IF="((AUTHENTICATION_MECHANISM=='NEGOTIATE_MEC') OR (AUTHENTICATION_MECHANISM=='PLAIN_MEC') OR (AUTHENTICATION_MECHANISM=='SCRAMSHA1_MEC')) AND (REQUIRED_AUTHENTICATION == 'true') AND (USE_EXISTING_CONNECTION == 'false')"
            GROUP="AUTHENTICATION"
        >
            <DEFAULT>""</DEFAULT>
        </PARAMETER>

        <PARAMETER
            FIELD="TEXT"
            NAME="COLLECTION"
            REQUIRED="true"
            NUM_ROW="17"
        >
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER
            NAME="DROP_COLLECTION_CREATE"
            FIELD="CHECK"
            NUM_ROW="17"
        >
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER
            NAME="DATA_ACTION"
            FIELD="CLOSED_LIST"
            NUM_ROW="21"
        >
            <ITEMS DEFAULT="INSERT">
                <ITEM NAME="INSERT" VALUE="INSERT" />
                <ITEM NAME="UPDATE" VALUE="UPDATE"/>
                <ITEM NAME="UPSERT" VALUE="UPSERT"/>
                <ITEM NAME="DELETE" VALUE="DELETE"/>
            </ITEMS>
        </PARAMETER>
        <PARAMETER
            NAME="SCHEMA_FLOW"
            FIELD="SCHEMA_TYPE"
            NUM_ROW="26"
        >
           <DEFAULT>""</DEFAULT>
        </PARAMETER>
         <PARAMETER
            FIELD="TEXT"
            NAME="DATA_NODE"
            NUM_ROW="31"
        >
            <DEFAULT>"data"</DEFAULT>
        </PARAMETER>
        <PARAMETER
            FIELD="TEXT"
            NAME="QUERY_NODE"
            NUM_ROW="31"
        >
            <DEFAULT>"query"</DEFAULT>
        </PARAMETER>
        <PARAMETER
            NAME="REMOVE_ROOT"
            FIELD="CHECK"
            NUM_ROW="36"
        >
            <DEFAULT>true</DEFAULT>
        </PARAMETER>
        <PARAMETER
            NAME="DIE_ON_ERROR"
            FIELD="CHECK"
            NUM_ROW="41">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>

	<PARAMETER FIELD="CHECK" NAME="QUERYOPTION_NOTIMEOUT"
            NUM_ROW="50" GROUP="ADVANCED_QUERY_OPTIONS">
            <DEFAULT>false</DEFAULT>
	</PARAMETER>

        <PARAMETER NAME="ROOT" FIELD="TABLE" REQUIRED="true"
            NUM_ROW="70" NB_LINES="3" SHOW="false">
            <ITEMS>
                <ITEM NAME="PATH" FIELD="TEXT" />
                <ITEM NAME="COLUMN" FIELD="TEXT" />
                <ITEM NAME="VALUE" FIELD="TEXT" />
                <ITEM NAME="ATTRIBUTE" FIELD="TEXT" />
                <ITEM NAME="ORDER" FIELD="TEXT" />
            </ITEMS>
        </PARAMETER>

        <PARAMETER NAME="GROUP" FIELD="TABLE" REQUIRED="false"
            NUM_ROW="80" NB_LINES="5" SHOW="false">
            <ITEMS>
                <ITEM NAME="PATH" FIELD="TEXT" />
                <ITEM NAME="COLUMN" FIELD="TEXT" />
                <ITEM NAME="VALUE" FIELD="TEXT" />
                <ITEM NAME="ATTRIBUTE" FIELD="TEXT" />
                <ITEM NAME="ORDER" FIELD="TEXT" />
            </ITEMS>
        </PARAMETER>

        <PARAMETER NAME="LOOP" FIELD="TABLE" REQUIRED="true"
            NUM_ROW="90" SHOW="false">
            <ITEMS>
                <ITEM NAME="PATH" FIELD="TEXT" />
                <ITEM NAME="COLUMN" FIELD="TEXT" />
                <ITEM NAME="VALUE" FIELD="TEXT" />
                <ITEM NAME="ATTRIBUTE" FIELD="TEXT" />
                <ITEM NAME="ORDER" FIELD="TEXT" />
            </ITEMS>
        </PARAMETER>
	</PARAMETERS>
	<CODEGENERATION>
		<IMPORTS>
            <IMPORT MODULE="mongo-java-driver-3.12.0.jar" MVN="mvn:org.mongodb/mongo-java-driver/3.12.0"  NAME="mongo-java-driver-3.12.0" REQUIRED_IF="(USE_EXISTING_CONNECTION == 'false') AND (API=='MONGODB')" />

            <IMPORT NAME="xom" MODULE="xom-1.2.7.jar" MVN="mvn:org.talend.libraries/xom-1.2.7/6.0.0"  REQUIRED="true" />
            <IMPORT NAME="commons_beanutils" MODULE="commons-beanutils-1.9.4.jar" MVN="mvn:commons-beanutils/commons-beanutils/1.9.4" REQUIRED="true" />
            <IMPORT NAME="commons_collections" MODULE="commons-collections-3.2.2.jar" MVN="mvn:commons-collections/commons-collections/3.2.2"  UrlPath="platform:/plugin/org.talend.libraries.apache.common/lib/commons-collections-3.2.2.jar" REQUIRED="true" />
            <IMPORT NAME="commons_lang" MODULE="commons-lang-2.6.jar" MVN="mvn:commons-lang/commons-lang/2.6"  UrlPath="platform:/plugin/org.talend.libraries.apache.common/lib/commons-lang-2.6.jar" REQUIRED="true" />
            <IMPORT NAME="commons_logging" MODULE="commons-logging-1.1.1.jar" MVN="mvn:org.talend.libraries/commons-logging-1.1.1/6.0.0"  UrlPath="platform:/base/plugins/org.apache.commons.logging_1.1.1.v201101211721.jar" REQUIRED="true" />
            <IMPORT NAME="ezmorph" MODULE="ezmorph-1.0.6.jar" MVN="mvn:org.talend.libraries/ezmorph-1.0.6/6.0.0"  UrlPath="platform:/plugin/org.talend.libraries.jackson/lib/ezmorph-1.0.6.jar" REQUIRED="true" />
            <IMPORT NAME="json-lib" MODULE="json-lib-2.4.6-talend.jar" MVN="mvn:net.sf.json-lib/json-lib/2.4.6-talend" REQUIRED="true" />
        </IMPORTS>
	</CODEGENERATION>
	<RETURNS>
		<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
	</RETURNS>
</COMPONENT>
